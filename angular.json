{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-sp-infra": {
      "projectType": "library",
      "root": "projects/ngx-sp-infra",
      "sourceRoot": "projects/ngx-sp-infra/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-sp-infra/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-sp-infra/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-sp-infra/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "karmaConfig": "projects/ngx-sp-infra/karma.conf.js",
            "tsConfig": "projects/ngx-sp-infra/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-sp-infra/**/*.ts",
              "projects/ngx-sp-infra/**/*.html"
            ]
          }
        }
      }
    },
    "ngx-sp-auth": {
      "projectType": "library",
      "root": "projects/ngx-sp-auth",
      "sourceRoot": "projects/ngx-sp-auth/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-sp-auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-sp-auth/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-sp-auth/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-sp-auth/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-sp-auth/**/*.ts",
              "projects/ngx-sp-auth/**/*.html"
            ]
          }
        }
      }
    },
    "components-showcase": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/components-showcase",
      "sourceRoot": "projects/components-showcase/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/components-showcase",
            "index": "projects/components-showcase/src/index.html",
            "browser": "projects/components-showcase/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/components-showcase/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/components-showcase/public"
              }
            ],
            "styles": [
              "projects/components-showcase/src/styles.scss",
              "dist/ngx-sp-infra/src/assets/styles/global.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "components-showcase:build:production"
            },
            "development": {
              "buildTarget": "components-showcase:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/components-showcase/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/components-showcase/public"
              }
            ],
            "styles": [
              "projects/components-showcase/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/components-showcase/**/*.ts",
              "projects/components-showcase/**/*.html"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
