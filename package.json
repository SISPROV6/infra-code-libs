{
  "name": "infra-code-libs",
  "version": "2.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:patch": "cd projects/ngx-sp-infra/src/scripts/ && node version.js patch && node tag.js && cd ../../../../ && ng test --watch=false --browsers=ChromeHeadless && node projects/ngx-sp-infra/src/scripts/commit.js",
    "build:minor": "cd projects/ngx-sp-infra/src/scripts/ && node version.js minor && node tag.js && cd ../../../../ && ng test --watch=false --browsers=ChromeHeadless && node projects/ngx-sp-infra/src/scripts/commit.js",
    "build:major": "cd projects/ngx-sp-infra/src/scripts/ && node version.js major && node tag.js && cd ../../../../ && ng test --watch=false --browsers=ChromeHeadless && node projects/ngx-sp-infra/src/scripts/commit.js",
    "deploy": "cd deploy/ && npm i && node index.js"
  },
  "files": [
    "projects/ngx-sp-infra/src/assets/styles",
    "dist"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "bootstrap": "^5.3.3",
    "ngx-bootstrap": "^18.1.3",
    "ngx-currency": "^18.0.0",
    "ngx-mask": "^18.0.4",
    "ngx-pagination": "^6.0.3",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.16",
    "@angular-eslint/builder": "18.4.3",
    "@angular-eslint/eslint-plugin": "18.4.3",
    "@angular-eslint/eslint-plugin-template": "18.4.3",
    "@angular-eslint/schematics": "18.4.3",
    "@angular-eslint/template-parser": "18.4.3",
    "@angular/cli": "^18.2.16",
    "@angular/compiler-cli": "^18.2.13",
    "@types/jasmine": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@typescript-eslint/types": "^8.28.0",
    "@typescript-eslint/utils": "^8.28.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.28.0"
  }
}
